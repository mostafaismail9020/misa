<action-definition id="com.sagia.backoffice.action.surveyreport.download"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/action-definition.xsd">
    <name>Excel Survey Report</name>
    <description>Action providing exporting data to Excel file</description>
    <author>hybris</author>
    <version>1.0</version>
    <actionClassName>com.sap.ibso.eservices.backoffice.actions.FinancialSurveyReportDownloadAction</actionClassName>

    <iconUri>icons/icon_action_export_excel_default.png</iconUri>
    <iconDisabledUri>icons/icon_action_export_excel_disabled.png</iconDisabledUri>
    <iconHoverUri>icons/icon_action_export_excel_hover.png</iconHoverUri>

    <settings>
        <setting key="confirmation.threshold" default-value="500" type="java.lang.Integer"/>
    </settings>

</action-definition>
